#include <stdio.h>

void main(){
    int n,i,sum=0;
    printf("Enter the number:");
    scanf("%d", &n);
    for( i=0; i<n; i++){
        if(n%i==0){
            sum = sum+i;
        }
    }
        if(sum==n)
        printf("Perfect Number");
        else
        printf("Not a Perfect Number");
}
